
//Practice environment for code
#include "samples/prototypes.h"
#include <getopt.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <stdbool.h>

int main(int argc, char *argv[]) {
    int d;
    char *s;
    int n = sscanf("69 abcdefghijklmnopqrstuvwxy", " %d %32s", &d, s);
    printf("d = %d\n", d);
    printf("s = %s\n", s);
    printf("n = %d\n", n);
    return 0;
}
